    "cy:run:second": "npx cypress run --spec cypress/e2e/1-getting-started/verifier_header.cy.js --browser electron --record --key 360739c3-df3f-4446-a3e9-695da66dbbaa",
    "cy:run:both": "npm run cy:run:first && npm run cy:run:second",

    "cy:anotherConfig": "npx cypress open --config-file cypress.config-2.js"
  },
