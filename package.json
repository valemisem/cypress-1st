  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run --spec 'cypress/e2e/1-getting-started/erifier_header.cy.js' ",
    "cy:dashboard": "npx cypress run --record --key 360739c3-df3f-4446-a3e9-695da66dbbaa",
